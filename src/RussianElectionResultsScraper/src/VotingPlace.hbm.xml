<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="RussianElectionResultsScraper" assembly="RussianElectionResultsScraper">

  <class name="VotingPlace" table="VotingPlace">

    <id name="Id"/>
    <property name="Name"/>
    <property name="FullName"/>
    <property name="Uri"/>
    <set name="Results" cascade="all">
      <key column="VotingPlaceId"/>
      <one-to-many class="VotingResult"/>
    </set>

  </class>

</hibernate-mapping>
