<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="RussianElectionResultsScraper" assembly="RussianElectionResultsScraper">

  <class name="WorkItem" table="WorkItem">

    <id name="Id">
      <column name="Id" sql-type="int" not-null="true"/>
      <generator class="identity" />
    </id>

    <property name="Uri">
      <column name="Uri" length="300" not-null="true" />
    </property>
    <property name="ProcessingStartedTimestamp"/>
    <property name="ParentVotingPlaceId"/>
    
  </class>

</hibernate-mapping>
